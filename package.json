{
  "name": "@donkeyclip/motorcortex-graphs",
  "version": "1.6.0",
  "description": "A plugin for creating graphs using MotorCortex",
  "main": "dist/motorcortex-graphs.cjs.js",
  "module": "dist/motorcortex-graphs.esm.js",
  "browser": "dist/motorcortex-graphs.umd.js",
  "author": "Donkeyclip (donkeyclip.com) <opensource@donkeyclip.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/donkeyclip/motorcortex-graphs"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint -c .eslintrc src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "rollup -c",
    "start": "npm run build:lib && concurrently -c \"cyan.bold,magenta.bold\" \"npm:build:lib -- -w\"  \"npm:start:demo\" ",
    "start:demo": "webpack serve --mode=development --config ./demo/webpack.config.js",
    "build:demo": "webpack --mode=production --config ./demo/webpack.config.js",
    "test": "HERE GOES YOUR TEST TASK",
    "test:prod": "npm run lint",
    "prepare": "husky install"
  },
  "keywords": [
    "motorcortex",
    "animation"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{json,md,yml,yaml,css}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "browserify": "^17.0.0",
    "jss": "^10.8.0"
  },
  "peerDependencies": {
    "@donkeyclip/motorcortex": "^7",
    "@donkeyclip/motorcortex-2dcam": ">= 0.0.15 < 1",
    "@donkeyclip/motorcortex-anime": ">= 2.1.16 < 3",
    "@donkeyclip/motorcortex-counter": ">= 1.1.0 < 2",
    "@donkeyclip/motorcortex-svgdraw": ">= 0.0.9 < 1"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/eslint-parser": "7.16.3",
    "@babel/plugin-syntax-jsx": "7.16.0",
    "@babel/plugin-transform-react-jsx": "7.16.0",
    "@babel/preset-env": "7.16.4",
    "@donkeyclip/motorcortex": "7.6.0",
    "@donkeyclip/motorcortex-2dcam": "0.0.16",
    "@donkeyclip/motorcortex-anime": "2.1.16",
    "@donkeyclip/motorcortex-counter": "1.1.1",
    "@donkeyclip/motorcortex-player": "2.4.0",
    "@donkeyclip/motorcortex-svgdraw": "0.0.10",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "8.0.3",
    "@size-limit/preset-big-lib": "6.0.4",
    "babel-loader": "8.2.3",
    "concurrently": "6.4.0",
    "core-js": "3.19.1",
    "css-loader": "6.5.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.1",
    "husky": "7.0.4",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "rollup": "2.60.0",
    "rollup-plugin-terser": "7.0.2",
    "semantic-release": "18.0.0",
    "size-limit": "6.0.4",
    "webpack": "5.64.1",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.5.0"
  }
}
